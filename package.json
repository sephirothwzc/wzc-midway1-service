{
  "name": "midway-financial_system",
  "version": "1.0.1",
  "description": "base",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "biguint-format": "^1.0.2",
    "bluebird": "^3.7.2",
    "dataloader": "^2.0.0",
    "date-fns": "^2.16.1",
    "egg-cache-9": "^1.1.2",
    "egg-cors": "^2.2.3",
    "egg-graphql": "^2.8.0",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.0",
    "egg-multipart": "^2.10.3",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.10.0",
    "flake-idgen": "^1.4.0",
    "graphql-scalars": "^1.2.6",
    "joi": "^17.2.1",
    "lodash": "^4.17.19",
    "midway": "^1.20.3",
    "mysql2": "^2.1.0",
    "sequelize": "5.21.13",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.33",
    "@types/flake-idgen": "^0.1.30",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.5.5",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "code-generator-wzc": "^1.0.66",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.1.0",
    "cross-env": "^6.0.0",
    "cz-conventional-changelog": "^3.2.0",
    "egg-ci": "^1.8.0",
    "eslint": "^7.8.0",
    "eslint-midway-contrib": "^1.20.1",
    "husky": "^4.2.5",
    "midway-bin": "1",
    "midway-mock": "1",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^8.3.0",
    "tslib": "^1.8.1",
    "typescript": "^3.5.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "commit": "git add . && git-cz && git push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "start": "egg-scripts start --daemon --title=midway-financial_system --framework=midway --ts",
    "stop": "egg-scripts stop --title=midway-financial_system",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "clean": "midway-bin clean",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "develop": "EGG_SERVER_ENV=develop yarn start",
    "stage": "EGG_SERVER_ENV=stage yarn start",
    "dev:changxindian": "EGG_SERVER_ENV=changxindian.local yarn dev",
    "develop:changxindian": "EGG_SERVER_ENV=changxindian.develop yarn start",
    "stage:changxindian": "EGG_SERVER_ENV=changxindian.stage yarn start",
    "start:changxindian": "EGG_SERVER_ENV=changxindian.prod yarn start",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "ci": "EGG_SERVER_ENV=develop npm run cov",
    "build": "midway-bin build -c && cp -rv src/config/keys dist/config/ && cp -rv src/app/graphql dist/app/graphql/",
    "autod": "midway-bin autod",
    "code": "node ./node_modules/code-generator-wzc/index.js",
    "db-migrate": "npx sequelize db:migrate",
    "db-migrate-undo": "npx sequelize db:migrate:undo",
    "db-migrate-undo-all": "npx sequelize db:migrate:undo:all",
    "db-seed": "npx sequelize db:seed:all",
    "db-seed-undo": "npx sequelize db:seed:undo",
    "db-seed-undo-all": "npx sequelize db:seed:undo:all"
  },
  "ci": {
    "version": "10"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "zhanchao.wu@icloud.com",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "commit-msg": "commitlint -e -V "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
